digraph EnglishWordDFA {
	rankdir=LR
	node [shape=circle]
	q0 [label="q0
(Start)" shape=circle]
	q1 [label="q1
(Accept)" shape=doublecircle]
	q2 [label="q2
(Reject)" shape=circle]
	q0 -> q1 [label="letter
(a-z, A-Z)"]
	q0 -> q2 [label="other
(digit, space, punct)"]
	q1 -> q1 [label="lowercase
(a-z)"]
	q1 -> q2 [label="other
(A-Z, digit, punct)"]
	q2 -> q2 [label=any]
}
